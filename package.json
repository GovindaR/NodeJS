    "babel-core": "^6.26.3",
    "express": "^4.16.4",
    "knex": "^0.15.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.6",
    "pg": "^7.6.1",
    "winston": "^3.1.0"
  }
